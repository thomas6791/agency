<!doctype html>
<html>
  <head>
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-127161482-1"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'UA-127161482-1');
    </script>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="google-site-verification" content="6n9VEVzrQsFYWwqlzP9SQj7DBG8Up8dgrNqBViIDOJU" />
    <!-- Use the title from a page's frontmatter if it has one -->
    <title><%= current_page.data.title %></title>
    <meta name="description" content="<%= current_page.data.description %>" />
    <%= stylesheet_link_tag "application" %>
    <%= javascript_include_tag "application" %>
    <%= favicon_tag 'images/favicon.png' %>
  </head>
  <body>
    <%= partial "layouts/navbar" %>
    <%= yield %>
  </body>
  <%= javascript_include_tag "navbar" %>
  <!-- Quantcast Choice. Consent Manager Tag -->
<%= javascript_include_tag "rgpd" %>
<!-- End Quantcast Choice. Consent Manager Tag -->
</html>
